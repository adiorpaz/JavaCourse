public class WomenClassExercise {
	
	//Attributes of an object
	public String name;
	public int age;
	public int realAge;
	

		public WomenClassExercise(String name, int age) {
			
		// Instance Variables
		this.name = name;
		
		//Condition fo the age of 30+
		if (age >= 30) {
			this.realAge = age + 5;
		}
		else {this.realAge = age;
	}
	this.age = age;
   }
}
